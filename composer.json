{
  "name": "thefrosty/wp-multi-network-plugins",
  "description": "Plugin lists for global multisite networks.",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-muplugin",
  "authors": [
    {
      "name": "Austin Passy",
      "email": "367897+thefrosty@users.noreply.github.com",
      "homepage": "https://austin.passy.co",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/thefrosty/wp-multi-network-plugins/issues",
    "forum": "https://github.com/thefrosty/wp-multi-network-plugins/discussions",
    "source": "https://github.com/thefrosty/wp-multi-network-plugins"
  },
  "require": {
    "php": "~8.3",
    "composer/installers": "^2.0",
    "symfony/http-foundation": "^7.3.4"
  },
  "require-dev": {
    "phpcompatibility/phpcompatibility-wp": "^2.1.0",
    "roots/wordpress": "^6.8",
    "slevomat/coding-standard": "^8.18",
    "squizlabs/php_codesniffer": "^3.9",
    "wp-coding-standards/wpcs": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "TheFrosty\\WpMultiNetworkPlugins\\": "src/"
    }
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --standard=phpcs.xml",
    "phpstan": "vendor/bin/phpstan analyse --memory-limit=1G",
    "rector:dry": "vendor/bin/rector process --dry-run",
    "rector": "vendor/bin/rector process"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "roots/wordpress-core-installer": true
    },
    "sort-packages": true
  }
}
